{
  "name": "serverest-api-test-automation",
  "version": "1.0.0",
  "description": "Projeto de automação de testes de API do ServeRest",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --e2e --headless",
    "test:electron": "cypress run --e2e --browser electron --headless",
    "test:chrome": "cypress run --e2e --browser chrome --headless",
    "test:firefox": "cypress run --e2e --browser firefox --headless",
    "test:webkit": "cypress run --e2e --browser webkit --headless",
    "test:post": "cypress run --e2e --spec 'cypress/e2e/post.usuarios.spec.cy.js' --headless",
    "test:post:electron": "cypress run --e2e --spec 'cypress/e2e/post.usuarios.spec.cy.js' --browser electron --headless",
    "test:get": "cypress run --e2e --spec 'cypress/e2e/get.usuarios.spec.cy.js' --headless",
    "test:get:electron": "cypress run --e2e --spec 'cypress/e2e/get.usuarios.spec.cy.js' --browser electron --headless",
    "test:all": "npm run test:post && npm run test:get",
    "test:all:electron": "npm run test:post:electron && npm run test:get:electron",
    "test:all:browsers": "npm run test:electron && npm run test:chrome && npm run test:firefox && npm run test:webkit",
    "cy:open": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^8.12.0",
    "cypress": "^13.6.3"
  }
}
